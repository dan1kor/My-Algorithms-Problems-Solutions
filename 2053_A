def solution(lens: list[int]) -> str:
    for i in range(len(lens) - 1):
        if lens[i] * 2 > lens[i + 1] and lens[i + 1] * 2 > lens[i]:
            return 'yes'
    return 'no'


def main():
    t = int(input())
    res = [0 for _ in range(t)]

    for i in range(t):
        n = int(input())
        *nums, = map(int, input().split())
        res[i] = solution(nums)

    print(*res, sep='\n')


main()
